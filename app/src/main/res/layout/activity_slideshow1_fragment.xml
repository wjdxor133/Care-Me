<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CEDCF8"
    tools:context=".fragment.Slideshow1Fragment">
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/animation_view1"
        android:layout_width="310dp"
        android:layout_height="280dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="120dp"
        app:lottie_autoPlay="true"
        app:lottie_fileName="1342-location.json"
        app:lottie_loop="true" />


    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/animation_view2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="70dp"
        android:background="@drawable/btn_slidshow_radius"
        app:lottie_autoPlay="true"
        app:lottie_fileName="10788-camera-motion.json"
        app:lottie_loop="true" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="483dp"
        android:layout_marginBottom="50dp"
        android:padding="5dp"
        android:src="@drawable/slideshow1_text" />


</RelativeLayout>